    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "radardeoficinas"
    },
    "web": {
      "favicon": "./assets/images/NearGarage.png"
    },
    "android": {
      "package": "radardeoficinas.apk"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}